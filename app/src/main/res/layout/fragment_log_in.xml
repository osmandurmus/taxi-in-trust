<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".LogInFragment">


    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/register"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="136dp" android:layout_marginTop="76dp"
            app:layout_constraintTop_toTopOf="parent" tools:text="@string/register"/>
    <Button
            android:text="Button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/login" android:layout_marginTop="100dp"
            app:layout_constraintTop_toBottomOf="@+id/register" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="136dp" tools:text="@string/login"/>

    <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Button"
            tools:layout_editor_absoluteX="174dp"
            tools:layout_editor_absoluteY="422dp" />
</androidx.constraintlayout.widget.ConstraintLayout>